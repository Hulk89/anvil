{
  "title": "sync mac & linux",
  "rules": [
    {
      "description": "ctrl-tab to cmd-tab",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "tab",
            "modifiers": {
              "mandatory": [ "left_control" ]
            }
          },
          "to": [
            {
              "key_code": "tab",
              "modifiers": [ "left_command" ]
            }
          ]
        }
      ]
    }
  ]
}
