// Modifiers
//
// Control:     ^
// Shift:       $
// Option:      ~
// Command:     @


{
    "^a" = "selectAll:";
    "^s" = "save:";

    "^z" = "undo:";
    "^x" = "cut:";
    "^c" = "copy:";
    "^v" = "paste:";

    "\UF729" = "moveToBeginningOfLine:";                    // Home
    "$\UF729" = "moveToBeginningOfLineAndModifySelection:";
    "\UF72B" = "moveToEndOfLine:";                          // End
    "$\UF72B" = "moveToEndOfLineAndModifySelection:";
}

