{
  "title": "lazylife custom",
  "rules": [
    {
      "description": "fn + escape to `",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "escape",
            "modifiers": {
              "mandatory": [ "fn" ],
              "optional": [ "any" ]
            }
          },
          "to": [
            {
              "key_code": "grave_accent_and_tilde"
            }
          ]
        }
      ]
    }
  ]
}
