{
    "title": "cursors",
    "rules": [
      {
        "description": "fn + i/j/k/l to arrows",
        "manipulators": [
          {
            "type": "basic",
            "from": {
              "key_code": "i",
              "modifiers": {
                "mandatory": [
                  "fn"
                ],
                "optional": [
                  "any"
                ]
              }
            },
            "to": [
              {
                "key_code": "up_arrow"
              }
            ]
          },
          {
            "type": "basic",
            "from": {
              "key_code": "j",
              "modifiers": {
